<script>
    $('#formdaily').submit(function (res) {
        res.preventDefault()
        let formData = new FormData(this)
        $.ajax({
            type: 'GET',
            url: '/Home/daily',
            data: formData,
            cache: false,
            contentType: false,
            processData: false,
            success: function (formdata) {
                $('#dedmaroz').empty();
                $('#dedmaroz').html(formdata);
                console.log($('#dedmaroz'))
            }


        })
    })
</script>